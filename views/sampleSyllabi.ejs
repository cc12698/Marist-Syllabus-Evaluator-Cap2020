<!DOCTYPE html>
<html>
  <head>
    <link rel="stylesheet" href="/css/signin.css">
    <link rel="stylesheet" href="/css/sampleSyllabi.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <link rel="icon" href="/img/favicon.ico" type="image/gif" sizes="16x16">

    <style media="screen">
      .contain{
        padding: 10px;
      }
    </style>
    <meta name="google" value="notranslate">
  </head>
  <body>
    <%- include('partials/header'); -%>
    <div class="contain">
      <div class="center-head">
        <span class="header">SAMPLE SYLLABI</span>
      </div>

      <% for(var i=0; i < syllabi.length; i++) { %>
        <div class="syl">
          <a href = "https://s3.us-south.cloud-object-storage.appdomain.cloud/sample-syl/<%= syllabi[i]['Key']%>" target="blank" class="samplesyl"><%= syllabi[i]['Key']%></a>
        </div>
      <% } %>
    </div>


    <%- include('partials/footer'); -%>
  </body>
    <script type = "text/javascript" src= "/js/sampleSyllabi.js"></script>
  <script src = "https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
</html>
